# Initializers
org.springframework.context.ApplicationContextInitializer=\
org.example.diveinspringboot.context.HelloWorldApplicationContextInitializer,\
org.example.diveinspringboot.context.AfterHelloWorldApplicationContextInitializer

# Application Listeners
org.springframework.context.ApplicationListener=\
org.example.diveinspringboot.event.AfterHelloWorldApplicationListener,\
org.example.diveinspringboot.event.HelloWorldApplicationListener,\
org.example.diveinspringboot.event.BeforeConfigFileApplicationListener

# Run Listeners
org.springframework.boot.SpringApplicationRunListener=\
org.example.diveinspringboot.run.HelloWorldRunListener